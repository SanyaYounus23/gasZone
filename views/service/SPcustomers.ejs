<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%-include('partial/SP header.ejs')%>
    <%-include('partial/SP slide.ejs')%>
    <main id="main" class="main">

        <div class="pagetitle">
          <h1>Recent Customers</h1>
          <nav>
            <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="/serviceproviderdashboard">Home</a></li>
              <li class="breadcrumb-item">Tables</li>
              <li class="breadcrumb-item active">Recent Customers</li>
            </ol>
          </nav>
        </div><!-- End Page Title -->
    
        <section class="section">
          <div class="row">
            <div class="col-lg-12">
    
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title">Recent Customers</h5>
                 
    
                  <!-- Table with stripped rows -->
                  <table class="table datatable">
                    <thead>
                      <tr>
                        <th>#</th>
                        <th>
                          Name
                        </th>
                        <th>Email </th>
                        <th>Contact</th>
                        <th>Location</th>
                        <th>Gender</th>
                        <th>Cylinder Type</th>
                       <th>Quantity</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% let count=1; cat.forEach(item => { %>
                        <tr>
                          <th scope="row"><%= count %></th>
                          <td><%= item.name %></td>
                          <td><%= item.email %></td>
                          <td><%= item.contact %></td>
                          <td><%= item.location %></td>
                          <td><%= item.gender %></td>
                          <td><%= item.cylinder %></td>
                          <td><%= item.quantity %></td>
                     
                      </td>
                      <td>
                      </i>
                        <i
                          class="bi bi-pencil-square"
                          style="color: green; cursor: pointer"
                          onclick="showUpdateForm('<%= item._id %>', '<%= item.name %>')"
                        ></i>
                        <i
                        class="bi bi-trash3"
                        style="color: red; cursor: pointer"
                        onclick="deleteCategory('<%= item._id %>')"
                      ></i>
                      </td>
                    </tr>
                        </tr>
                      <% count++; }); %>
                    </tbody>
                </table>
            </div>

            <!-- ================= New Customers ================ -->
          
        </div>
    </div>
</div>
    
                </div>
              </div>
    
            </div>
          </div>
        </section>
    
      </main><!-- End #main -->   


    <%-include('partial/SP footer.ejs')%> 
</body>
</html>